  <data name="sqlSelectCommand1.CommandText" xml:space="preserve">
    <value>SELECT        a.ID, a.PRI_ID, a.Description, a.Original, 
isnull((select sum(h.AdditionalUse_Area) from PROJ_LD_RawLandHolding h
join PROJ_HEADER ph on ph.pri_id = h.PRI_ID where h.PROJ_LD_AdditionalUseArea_ID = a.ID and ph.pri_status != 'X' ),0)[ProjectAllocated],
isnull((select sum(h.AdditionalUse_Area) from EST_LD_RawLandHolding h join est_header eh on eh.ID = h.EST_ID where h.PROJ_LD_AdditionalUseArea_ID = a.ID and eh.Est_Status not in('C','X') ),0)[EstimateAllocated],  
isnull(a.Original,0) - isnull((select sum(h.AdditionalUse_Area) from PROJ_LD_RawLandHolding h where h.PROJ_LD_AdditionalUseArea_ID = a.ID),0) [AreaRemaining],Comment
FROM            PROJ_LD_AdditionalUseArea a
WHERE        (PRI_ID = @PRI_ID)</value>
  </data>
