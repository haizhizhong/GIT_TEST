  <data name="sqlInsertCommand1.CommandText" xml:space="preserve">
    <value>INSERT INTO [PROJ_LD_AdditionalUseArea] ([PRI_ID], [Description], [Original], [Comment]) VALUES (@PRI_ID, @Description, @Original, @Comment);
SELECT ID, PRI_ID, Description, Original, ISNULL((SELECT SUM(h.AdditionalUse_Area) AS Expr1 FROM PROJ_LD_RawLandHolding AS h INNER JOIN PROJ_HEADER AS ph ON ph.pri_id = h.PRI_ID WHERE (h.PROJ_LD_AdditionalUseArea_ID = a.ID) AND (ph.pri_status &lt;&gt; 'X')), 0) AS ProjectAllocated, ISNULL((SELECT SUM(h.AdditionalUse_Area) AS Expr1 FROM EST_LD_RawLandHolding AS h INNER JOIN EST_Header AS eh ON eh.ID = h.EST_ID WHERE (h.PROJ_LD_AdditionalUseArea_ID = a.ID) AND (eh.Est_Status NOT IN ('C', 'X'))), 0) AS EstimateAllocated, ISNULL(Original, 0) - ISNULL((SELECT SUM(AdditionalUse_Area) AS Expr1 FROM PROJ_LD_RawLandHolding AS h WHERE (PROJ_LD_AdditionalUseArea_ID = a.ID)), 0) AS AreaRemaining, Comment FROM PROJ_LD_AdditionalUseArea AS a WHERE (ID = SCOPE_IDENTITY())</value>
  </data>
