  <data name="sqlUpdateCommand1.CommandText" xml:space="preserve">
    <value>UPDATE [PROJ_LD_AdditionalUseArea] SET [PRI_ID] = @PRI_ID, [Description] = @Description, [Original] = @Original, [Comment] = @Comment WHERE (([ID] = @Original_ID) AND ((@IsNull_PRI_ID = 1 AND [PRI_ID] IS NULL) OR ([PRI_ID] = @Original_PRI_ID)) AND ((@IsNull_Description = 1 AND [Description] IS NULL) OR ([Description] = @Original_Description)) AND ((@IsNull_Original = 1 AND [Original] IS NULL) OR ([Original] = @Original_Original)));
SELECT ID, PRI_ID, Description, Original, ISNULL((SELECT SUM(h.AdditionalUse_Area) AS Expr1 FROM PROJ_LD_RawLandHolding AS h INNER JOIN PROJ_HEADER AS ph ON ph.pri_id = h.PRI_ID WHERE (h.PROJ_LD_AdditionalUseArea_ID = a.ID) AND (ph.pri_status &lt;&gt; 'X')), 0) AS ProjectAllocated, ISNULL((SELECT SUM(h.AdditionalUse_Area) AS Expr1 FROM EST_LD_RawLandHolding AS h INNER JOIN EST_Header AS eh ON eh.ID = h.EST_ID WHERE (h.PROJ_LD_AdditionalUseArea_ID = a.ID) AND (eh.Est_Status NOT IN ('C', 'X'))), 0) AS EstimateAllocated, ISNULL(Original, 0) - ISNULL((SELECT SUM(AdditionalUse_Area) AS Expr1 FROM PROJ_LD_RawLandHolding AS h WHERE (PROJ_LD_AdditionalUseArea_ID = a.ID)), 0) AS AreaRemaining, Comment FROM PROJ_LD_AdditionalUseArea AS a WHERE (ID = @ID)</value>
